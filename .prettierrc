{
	"useTabs": false,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"tabWidth": 2,
	"semi": true,
	"svelteIndentScriptAndStyle": false,
	"svelteSortOrder": "markup-scripts-styles-options",
	"svelteStrictMode": false
}
